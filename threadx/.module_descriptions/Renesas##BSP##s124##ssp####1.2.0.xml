			 <option display="32" id="config.bsp.s124.ssp.OFS0.iwdt_divisor.32" value="3 &lt;&lt; 4"/>
			 <option display="64" id="config.bsp.s124.ssp.OFS0.iwdt_divisor.64" value="4 &lt;&lt; 4"/>
			 <option display="128" id="config.bsp.s124.ssp.OFS0.iwdt_divisor.128" value="15 &lt;&lt; 4"/>
			 <option display="256" id="config.bsp.s124.ssp.OFS0.iwdt_divisor.256" value="5 &lt;&lt; 4"/>
		</property>			
		<property default="config.bsp.s124.ssp.OFS0.iwdt_window_end.0" display="     IWDT Window End Position" id="config.bsp.s124.ssp.OFS0.iwdt_window_end">
			 <option display="75%" id="config.bsp.s124.ssp.OFS0.iwdt_window_end.75" value="0 &lt;&lt; 8"/>
			 <option display="50%" id="config.bsp.s124.ssp.OFS0.iwdt_window_end.50" value="1 &lt;&lt; 8"/>
