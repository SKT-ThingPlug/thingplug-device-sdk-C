			 <option display="IWDT is automatically activated after a reset (Autostart mode)" id="config.bsp.s124.ssp.OFS0.iwdt_start_mode.enabled" value="0 &lt;&lt; 1"/>
		</property>
		<property default="config.bsp.s124.ssp.OFS0.iwdt_timeout.2048" display="     IWDT Timeout Period" id="config.bsp.s124.ssp.OFS0.iwdt_timeout">
			 <option display="128 cycles" id="config.bsp.s124.ssp.OFS0.iwdt_timeout.128" value="0 &lt;&lt; 2"/>
			 <option display="512 cycles" id="config.bsp.s124.ssp.OFS0.iwdt_timeout.512" value="1 &lt;&lt; 2"/>
			 <option display="1024 cycles" id="config.bsp.s124.ssp.OFS0.iwdt_timeout.1024" value="2 &lt;&lt; 2"/>
			 <option display="2048 cycles" id="config.bsp.s124.ssp.OFS0.iwdt_timeout.2048" value="3 &lt;&lt; 2"/>
		</property>	
		<property default="config.bsp.s124.ssp.OFS0.iwdt_divisor.128" display="     IWDT Dedicated Clock Frequency Divisor" id="config.bsp.s124.ssp.OFS0.iwdt_divisor">
