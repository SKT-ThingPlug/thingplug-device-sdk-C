<?xml version="1.0" encoding="UTF-8"?>
<synergyModuleDescription>
    <config id="config.bsp.s124.ssp" path="ssp_cfg/bsp/bsp_mcu_family_cfg.h" version="0">
       <property default="config.bsp.s124.ssp.OFS0_Register.default" display="OFS0 register settings" id="config.bsp.s124.ssp.OFS0">
			<option display="Select fields below" id="config.bsp.s124.ssp.OFS0_Register.default" value="(0)"/>
        </property>
		<property default="config.bsp.s124.ssp.OFS0.iwdt_start_mode.disabled" display="     IWDT Start Mode" id="config.bsp.s124.ssp.OFS0.iwdt_start_mode">
