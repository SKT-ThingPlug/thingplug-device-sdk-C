		<property default="config.driver.adc.param_checking_enable.enabled" display="Parameter Checking" id="config.driver.adc.param_checking_enable">
			 <option display="BSP" id="config.driver.adc.param_checking_enable.bsp" value="(BSP_CFG_PARAM_CHECKING_ENABLE)"/>
			<option display="Enabled" id="config.driver.adc.param_checking_enable.enabled" value="(1)"/>
			<option display="Disabled" id="config.driver.adc.param_checking_enable.disabled" value="(0)"/>
		</property>
		<content>#define ADC_CFG_PARAM_CHECKING_ENABLE ${config.driver.adc.param_checking_enable}
</content>
	</config>
	<module config="config.driver.adc" display="Driver|Analog|${module.driver.adc.name} ADC Driver on r_adc" id="module.driver.adc_on_adc" version="1">
        <constraint display="Requires ADC Normal/Group A (Scan End) Interrupt enabled">
          ("${module.driver.adc.p_callback}" === "NULL") || ("${module.driver.adc.scan_end_ipl}" != "board.icu.common.irq.disabled") ||("${module.driver.adc.mode}" != "module.driver.adc.mode.mode_single_scan")
        </constraint>
        <constraint display="Requires ADC Group B (Scan End Group B) and Normal/Group A (SCAN_END) Interrupt enabled">
            (("${module.driver.adc.p_callback}" === "NULL") ||("${module.driver.adc.mode}" != "module.driver.adc.mode.mode_group_scan")) || (("${module.driver.adc.scan_end_b_ipl}" != "board.icu.common.irq.disabled") &amp;&amp; ("${module.driver.adc.scan_end_ipl}" != "board.icu.common.irq.disabled"))
         </constraint>
         <constraint display="Requires callback to be not NULL in Group Mode">
            (("${module.driver.adc.p_callback}" != "NULL") ||("${module.driver.adc.mode}" != "module.driver.adc.mode.mode_group_scan"))
         </constraint>
         <constraint display="Requires callback to be NULL in Continuous Mode">
            (("${module.driver.adc.p_callback}" == "NULL") ||("${module.driver.adc.mode}" != "module.driver.adc.mode.mode_continuous_scan"))
         </constraint>
		<constraint display="Unique name required for each instance">
       ("${interface.driver.adc.${module.driver.adc.name}}" === "1")
	    </constraint>
        <provides interface="interface.driver.adc" />
        <provides interface="interface.driver.adc_on_adc" />
		<provides interface="interface.driver.adc.${module.driver.adc.name}" />
        <property default="g_adc${_instance}" display="Name" id="module.driver.adc.name">
			<constraint display="Name must be a valid C symbol">testSymbol("${module.driver.adc.name}")</constraint>
        </property>
		<property default="0" display="Unit" id="module.driver.adc.unit">
