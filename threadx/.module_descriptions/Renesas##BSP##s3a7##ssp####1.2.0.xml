		<property id="config.bsp.s3a7.ssp.mpu_pc0_enable" display="MPU - Enable or disable PC Region 0" default="config.bsp.s3a7.ssp.mpu_pc0_enable.disabled">
            <option id="config.bsp.s3a7.ssp.mpu_pc0_enable.enabled" display="Enabled" value="0" />
            <option id="config.bsp.s3a7.ssp.mpu_pc0_enable.disabled" display="Disabled" value="1" />
        </property>		
        <property id="config.bsp.s3a7.ssp.mpu_pc0_start" display="MPU - PC0 Start" default="0x000FFFFC" macro="BSP_CFG_ROM_REG_MPU_PC0_START">  
            <constraint display="Value must be an integer between 0 and 0x000FFFFC">testInteger("${config.bsp.s3a7.ssp.mpu_pc0_start}") &amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_pc0_start}" &gt;= 0x00000000) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_pc0_start}" &lt;= 0x000FFFFC))
			</constraint>		
        </property>
        <property id="config.bsp.s3a7.ssp.mpu_pc0_end" display="MPU - PC0 End" default="0x000FFFFF" macro="BSP_CFG_ROM_REG_MPU_PC0_END">
            <constraint display="Value must be an integer between 3 and 0x000FFFFF">testInteger("${config.bsp.s3a7.ssp.mpu_pc0_end}") &amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_pc0_end}" &gt;= 3) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_pc0_end}" &lt;= 0x000FFFFF))
			</constraint>		
        </property>
        <property id="config.bsp.s3a7.ssp.mpu_pc1_enable" display="MPU - Enable or disable PC Region 1" default="config.bsp.s3a7.ssp.mpu_pc1_enable.disabled">
            <option id="config.bsp.s3a7.ssp.mpu_pc1_enable.enabled" display="Enabled" value="0" />
            <option id="config.bsp.s3a7.ssp.mpu_pc1_enable.disabled" display="Disabled" value="1" />
        </property>		
        <property id="config.bsp.s3a7.ssp.mpu_pc1_start" display="MPU - PC1 Start" default="0x000FFFFC" macro="BSP_CFG_ROM_REG_MPU_PC1_START">
            <constraint display="Value must be an integer between 0 and 0x000FFFFC">testInteger("${config.bsp.s3a7.ssp.mpu_pc1_start}") &amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_pc1_start}" &gt;= 0x00000000) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_pc1_start}" &lt;= 0x000FFFFC))
			</constraint>		
        </property>
        <property id="config.bsp.s3a7.ssp.mpu_pc1_end" display="MPU - PC1 End" default="0x000FFFFF" macro="BSP_CFG_ROM_REG_MPU_PC1_END">
             <constraint display="Value must be an integer between 3 and 0x000FFFFF">testInteger("${config.bsp.s3a7.ssp.mpu_pc1_end}")&amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_pc1_end}" &gt;= 3) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_pc1_end}" &lt;= 0x000FFFFF))
			 </constraint>		
        </property>
       <property id="config.bsp.s3a7.ssp.mpu_reg0_enable" display="MPU - Enable or disable Memory Region 0" default="config.bsp.s3a7.ssp.mpu_reg0_enable.disabled">
            <option id="config.bsp.s3a7.ssp.mpu_reg0_enable.enabled" display="Enabled" value="0" />
            <option id="config.bsp.s3a7.ssp.mpu_reg0_enable.disabled" display="Disabled" value="1" />
        </property>		
        <property id="config.bsp.s3a7.ssp.mpu_reg0_start" display="MPU - Memory Region 0 Start" default="0x000FFFFC" macro="BSP_CFG_ROM_REG_MPU_REGION0_START">
            <constraint display="Value must be an integer between 0 and 0x000FFFFC">testInteger("${config.bsp.s3a7.ssp.mpu_reg0_start}") &amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_reg0_start}" &gt;= 0x00000000) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_reg0_start}" &lt;= 0x000FFFFC))
			</constraint>				
        </property>
        <property id="config.bsp.s3a7.ssp.mpu_reg0_end" display="MPU - Memory Region 0 End" default="0x000FFFFF" macro="BSP_CFG_ROM_REG_MPU_REGION0_END">
            <constraint display="Value must be an integer between 3 and 0x000FFFFF">testInteger("${config.bsp.s3a7.ssp.mpu_reg0_end}") &amp;&amp;
			(("${config.bsp.s3a7.ssp.mpu_reg0_end}" &gt;= 3) &amp;&amp;("${config.bsp.s3a7.ssp.mpu_reg0_end}" &lt;= 0x000FFFFF))
			</constraint>				
        </property>
        <content>
            #include "bsp_mcu_device_pn_cfg.h"
            #include "bsp_mcu_device_cfg.h"
            #include "../../../synergy/ssp/src/bsp/mcu/s3a7/bsp_mcu_info.h"
            #include "bsp_clock_cfg.h"
            #define BSP_MCU_GROUP_S3A7 (1)
            #define BSP_LOCO_HZ                 (32768)
            #define BSP_MOCO_HZ                 (8000000)
            #define BSP_SUB_CLOCK_HZ            (32768)
            #if   BSP_CFG_HOCO_FREQUENCY == 0
                #define BSP_HOCO_HZ             (24000000)
            #elif BSP_CFG_HOCO_FREQUENCY == 2
                #define BSP_HOCO_HZ             (32000000)
            #elif BSP_CFG_HOCO_FREQUENCY == 4
                #define BSP_HOCO_HZ             (48000000)
            #elif BSP_CFG_HOCO_FREQUENCY == 5
                #define BSP_HOCO_HZ             (64000000)
            #else
                #error "Invalid HOCO frequency chosen (BSP_CFG_HOCO_FREQUENCY) in bsp_clock_cfg.h"
            #endif
            #define BSP_CORTEX_VECTOR_TABLE_ENTRIES    (16U)
            #define BSP_VECTOR_TABLE_MAX_ENTRIES       (80U)
            #define BSP_MCU_VBATT_SUPPORT       (1)

            #define OFS_SEQ1 0xA001A001 | (${config.bsp.s3a7.ssp.OFS0.iwdt_start_mode}) | (${config.bsp.s3a7.ssp.OFS0.iwdt_timeout})
            #define OFS_SEQ2 (${config.bsp.s3a7.ssp.OFS0.iwdt_divisor}) | (${config.bsp.s3a7.ssp.OFS0.iwdt_window_end}) | (${config.bsp.s3a7.ssp.OFS0.iwdt_window_start})
            #define OFS_SEQ3 (${config.bsp.s3a7.ssp.OFS0.iwdt_reset_interrupt}) | (${config.bsp.s3a7.ssp.OFS0.iwdt_stop_control}) | (${config.bsp.s3a7.ssp.OFS0.wdt_start_mode})
            #define OFS_SEQ4 (${config.bsp.s3a7.ssp.OFS0.wdt_timeout}) |(${config.bsp.s3a7.ssp.OFS0.wdt_divisor}) | (${config.bsp.s3a7.ssp.OFS0.wdt_window_end}) | (${config.bsp.s3a7.ssp.OFS0.wdt_window_start})
            #define OFS_SEQ5 (${config.bsp.s3a7.ssp.OFS0.wdt_reset_interrupt}) | (${config.bsp.s3a7.ssp.OFS0.wdt_stop_control})           
            #define BSP_CFG_ROM_REG_OFS0 (OFS_SEQ1 | OFS_SEQ2 | OFS_SEQ3 | OFS_SEQ4 | OFS_SEQ5)           
            #define BSP_CFG_ROM_REG_OFS1 (0xFFFFFEC3 | (${config.bsp.s3a7.ssp.OFS1.voltage_detection0.start}) | (${config.bsp.s3a7.ssp.OFS1.voltage_detection0_level}) | (${config.bsp.s3a7.ssp.OFS1.hoco_osc}))						
            #define BSP_CFG_ROM_REG_MPU_PC0_ENABLE (${config.bsp.s3a7.ssp.mpu_pc0_enable})
            #define BSP_CFG_ROM_REG_MPU_PC0_START (${config.bsp.s3a7.ssp.mpu_pc0_start})
            #define BSP_CFG_ROM_REG_MPU_PC0_END (${config.bsp.s3a7.ssp.mpu_pc0_end})
            #define BSP_CFG_ROM_REG_MPU_PC1_ENABLE (${config.bsp.s3a7.ssp.mpu_pc1_enable})
            #define BSP_CFG_ROM_REG_MPU_PC1_START (${config.bsp.s3a7.ssp.mpu_pc1_start})
            #define BSP_CFG_ROM_REG_MPU_PC1_END (${config.bsp.s3a7.ssp.mpu_pc1_end})
            #define BSP_CFG_ROM_REG_MPU_REGION0_ENABLE (${config.bsp.s3a7.ssp.mpu_reg0_enable})
            #define BSP_CFG_ROM_REG_MPU_REGION0_START (${config.bsp.s3a7.ssp.mpu_reg0_start})
            #define BSP_CFG_ROM_REG_MPU_REGION0_END (${config.bsp.s3a7.ssp.mpu_reg0_end})
        </content>
    </config>   
    <bsp id="bsp.s3a7.ssp" config="config.bsp.s3a7.ssp" display="S3A7 Family" version="0">
        <clock>
            <node id="board.clock.xtal.freq" xhint="0" yhint="0" default="12000000" tooltip="Click to edit the XTAL frequency.">
                <option id="_edit" display="XTAL ${value}Hz" macroName="BSP_CFG_XTAL_HZ" macroValue="(${value})" />
            </node>
            <node id="board.clock.pll.source" xhint="1" yhint="1" default="board.clock.pll.source.xtal">
                <option id="board.clock.pll.source.xtal" display="PLL Src: XTAL" input="board.clock.xtal.freq" macroName="BSP_CFG_PLL_SOURCE" macroValue="(CGC_CLOCK_MAIN_OSC)" />
            </node>
            <node id="board.clock.hoco.freq" xhint="0" yhint="6" default="board.clock.hoco.freq.24m">
                <option id="board.clock.hoco.freq.24m" display="HOCO 24MHz" mul="24000000" macroName="BSP_CFG_HOCO_FREQUENCY" macroValue="(0)" />
                <option id="board.clock.hoco.freq.32m" display="HOCO 32MHz" mul="32000000" macroName="BSP_CFG_HOCO_FREQUENCY" macroValue="(2)" />
                <option id="board.clock.hoco.freq.48m" display="HOCO 48MHz" mul="48000000" macroName="BSP_CFG_HOCO_FREQUENCY" macroValue="(4)" />
                <option id="board.clock.hoco.freq.64m" display="HOCO 64MHz" mul="64000000" macroName="BSP_CFG_HOCO_FREQUENCY" macroValue="(5)" />
            </node>
            <node id="board.clock.loco.freq" xhint="0" yhint="7" default="board.clock.loco.freq.32768">
                <option id="board.clock.loco.freq.32768" display="LOCO 32768Hz" mul="32768" />
