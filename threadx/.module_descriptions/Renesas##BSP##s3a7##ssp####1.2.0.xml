		<property default="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level.190" display="     Voltage Detection 0 Level" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level">
			 <option display="3.84 V" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level.384" value="0 &lt;&lt; 3"/>
			 <option display="2.82 V" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level.282" value="1 &lt;&lt; 3"/>
			 <option display="2.51 V" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level.251" value="2 &lt;&lt; 3"/>
			 <option display="1.90 V" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0_level.190" value="3 &lt;&lt; 3"/>
