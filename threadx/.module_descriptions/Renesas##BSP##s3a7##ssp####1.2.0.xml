       <property default="config.bsp.s3a7.ssp.OFS1_Register.default" display="OFS1 register settings" id="config.bsp.s3a7.ssp.OFS1">
			<option display="Select fields below" id="config.bsp.s3a7.ssp.OFS1_Register.default" value="(0)"/>
        </property>		
		<property default="config.bsp.s3a7.ssp.OFS1.voltage_detection0.start.disabled" display="     Voltage Detection 0 Circuit Start" id="config.bsp.s3a7.ssp.OFS1.voltage_detection0.start">
