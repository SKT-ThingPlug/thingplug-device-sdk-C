			 <option display="HOCO oscillation is enabled after reset" id="config.bsp.s3a7.ssp.OFS1.hoco_osc.enabled" value="0 &lt;&lt; 8"/>
			 <option display="HOCO oscillation is disabled after reset" id="config.bsp.s3a7.ssp.OFS1.hoco_osc.disabled" value="1 &lt;&lt; 8"/>
