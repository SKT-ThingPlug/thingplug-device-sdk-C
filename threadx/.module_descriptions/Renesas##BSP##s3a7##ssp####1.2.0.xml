		<property default="config.bsp.s3a7.ssp.OFS0.wdt_window_end.0" display="     WDT Window End Position" id="config.bsp.s3a7.ssp.OFS0.wdt_window_end">
			 <option display="75%" id="config.bsp.s3a7.ssp.OFS0.wdt_window_end.75" value="0 &lt;&lt; 24"/>
			 <option display="50%" id="config.bsp.s3a7.ssp.OFS0.wdt_window_end.50" value="1 &lt;&lt; 24"/>
